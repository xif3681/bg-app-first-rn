{
  "name": "smart-location-rn",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output release_ios/main.jsbundle --assets-dest release_ios/",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "push-ios": "code-push release-react smartlocation-ios ios --mandatory --targetBinaryVersion 1.6.3",
    "public-ios": "code-push promote smartlocation-ios Staging Production",
    "show-ios": "code-push deployment ls smartlocation-ios -k",
    "push-android": "code-push release-react smartlocation-Android android --mandatory --targetBinaryVersion 1.6.3",
    "public-android": "code-push promote smartlocation-Android Staging Production",
    "show-android": "code-push deployment ls smartlocation-Android -k"
  },
